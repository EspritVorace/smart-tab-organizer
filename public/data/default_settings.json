{
  "globalGroupingEnabled": true,
  "globalDeduplicationEnabled": true,
  "notifyOnGrouping": true,
  "notifyOnDeduplication": true,
  "domainRules": [
    { "id": "rule-jira", "enabled": true, "domainFilter": "*.atlassian.net", "label": "Jira/Atlassian", "titleParsingRegEx": "\\[([A-Z]+-\\d+)\\]", "urlParsingRegEx": "browse/([A-Z]+-\\d+)", "groupNameSource": "smart_label", "deduplicationMatchMode": "exact", "color": "blue", "deduplicationEnabled": true, "presetId": "jira-ticket" },
    { "id": "rule-gitlab", "enabled": true, "domainFilter": "gitlab.com", "label": "GitLab", "titleParsingRegEx": "(.+?)\\s*\\|\\s*Trello", "urlParsingRegEx": "gitlab\\.com/([^/]+/[^/]+)", "groupNameSource": "smart_label", "deduplicationMatchMode": "exact", "color": "orange", "deduplicationEnabled": true, "presetId": "gitlab-project" },
    { "id": "rule-github", "enabled": true, "domainFilter": "github.com", "label": "GitHub", "titleParsingRegEx": "(.+?)\\s*Â·\\s*Issue\\s*#(\\d+)", "urlParsingRegEx": "issues/(\\d+)", "groupNameSource": "smart_preset", "deduplicationMatchMode": "exact", "color": "purple", "deduplicationEnabled": true, "presetId": "github-issue" },
    { "id": "rule-trello", "enabled": true, "domainFilter": "trello.com", "label": "Trello", "titleParsingRegEx": "(.+?)\\s*\\|\\s*Trello", "urlParsingRegEx": "trello\\.com/b/[^/]+/(.+)", "groupNameSource": "smart_label", "deduplicationMatchMode": "exact", "color": "cyan", "deduplicationEnabled": true, "presetId": "trello-board" },
    { "id": "rule-asana", "enabled": true, "domainFilter": "app.asana.com", "label": "Asana", "titleParsingRegEx": "", "urlParsingRegEx": "app\\.asana\\.com/0/([^/]+)", "groupNameSource": "smart_label", "deduplicationMatchMode": "exact", "color": "pink", "deduplicationEnabled": true, "presetId": "asana-task" },
    { "id": "rule-zendesk", "enabled": true, "domainFilter": "*.zendesk.com", "label": "Zendesk", "titleParsingRegEx": "Ticket #(\\d+)", "urlParsingRegEx": "tickets/(\\d+)", "groupNameSource": "title", "deduplicationMatchMode": "exact", "color": "green", "deduplicationEnabled": true, "presetId": null },
    { "id": "rule-servicenow", "enabled": true, "domainFilter": "*.service-now.com", "label": "ServiceNow", "titleParsingRegEx": "(INC\\d+|CHG\\d+|REQ\\d+|RITM\\d+)", "urlParsingRegEx": "(INC\\d+|CHG\\d+|REQ\\d+|RITM\\d+)", "groupNameSource": "title", "deduplicationMatchMode": "exact", "color": "yellow", "deduplicationEnabled": true, "presetId": null }
  ]
}